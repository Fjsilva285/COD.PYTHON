import matplotlib.pyplot as plt

paizes = 'Asia', 'Africa', 'Europe', 'North America', 'South America', 'Australia'
populacao = [59.69, 16, 9.94, 7.79, 5.68, 0.54]

figureObject, axesObject = plt.subplots()

axesObject.pie(
    populacao,
    labels=paizes,
    autopct='%1.2f',
    startangle=90
    )
axesObject.axis('equal')
    
plt.show()
